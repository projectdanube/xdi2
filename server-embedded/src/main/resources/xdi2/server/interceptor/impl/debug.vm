<!doctype html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<style type="text/css">
body { font-family: Calibri, Arial, Helvetica, sans-serif; font-size: 12pt; padding: 20px; }
a { text-decoration: none; color: #540dd8; border-bottom: 3px solid #540dd8; }
th { font-weight: bold; }
td { margin: 5px; padding: 5px; border: 2px solid black; }
hr { color: black; background-color: black; height: 3px; }
h1 { font-size: 1.5em; }
</style>
<title>XDI2 Server Status</title>
</head>
<body>
<table>
<tr>
<th>
<img width="64" height="64" style="float: left; margin-right: 20px;" src="data:image/png;base64,">
</th>
<th>
<h1 style="float: left">Server Status</h1>
</th>
<th>
<!--<form action="" method="post">
	<input type="hidden" name="cmd" value="reload">
	<input type="submit" value="reload">
</form>-->
</th>
</tr>
</table>
<hr style="clear: both" noshade>
<h1>General Information:</h1>
<table>
	<tr><th>Servlet Class:</th><td>$endpointservlet.class.canonicalName</td>
		<td rowspan="4" valign="top">
			<table>
				<tr>
				<th>Interceptors</th>
				</tr>
				<tr>
				<td valign="top">
				#foreach($interceptor in $endpointservlet.interceptors) 
					$interceptor.class.canonicalName<br>
				#end
				</td>
			    </tr>
			</table>
		</td>
	</tr>
	<tr><th>Startup Time:</th><td>$endpointservlet.getStartupAsString()</td></tr>
	<tr><th>URI:</th><td>$requestinfo.uri</td></tr>
	<tr><th>Parser:</th><td>$parser.class.canonicalName</td></tr>
</table>
<hr style="clear: both" noshade>
<h1>Active Plugins:</h1>
<table>
#foreach($pluginfile in $pluginfiles)
	<tr>
	<th>Plugin:</th>
    <td>$pluginfile.getAbsolutePath()</td>
    </tr>
#end
</table>
<hr style="clear: both" noshade>
<h1>Mounted MessagingTargets:</h1>
<table>
	<tr>
	<th>Path</th>
	<th>MessagingTarget</th>
	<th>Owner Authority</th>
	<th>Contributors</th>
	<th>Interceptors</th>
	</tr>
#foreach($messagingtargetbypath in $messagingtargetsbypath.entrySet())
	<tr>
    <td valign="top">
    	<a href="$requestinfo.contextPath$requestinfo.servletPath$messagingtargetbypath.key">
    		$requestinfo.contextPath$requestinfo.servletPath$messagingtargetbypath.key
    	</a>
		<p><form action="" method="post">
			<input type="hidden" name="cmd" value="unmount_messaging_target">
			<input type="hidden" name="messagingtargetpath" value="$messagingtargetbypath.key">
			<input type="submit" value="unmount">
		</form></p>
    </td>
    <td valign="top">
    	$messagingtargetbypath.value.class.canonicalName
    	#if($messagingtargetbypath.value.class.canonicalName=="xdi2.messaging.target.impl.graph.GraphMessagingTarget")
    		<br><span style="color:gray">$messagingtargetbypath.value.graph.class.canonicalName</span>
	    	#if($messagingtargetbypath.value.graph.class.canonicalName=="xdi2.core.impl.file.FileGraph")
	    		<br>[path] <span style="color:gray">$messagingtargetbypath.value.graph.path</span>
	    		<br>[mimeType] <span style="color:gray">$messagingtargetbypath.value.graph.mimeType</span>
	    	#end
	    	#if($messagingtargetbypath.value.graph.class.canonicalName=="xdi2.core.impl.keyvalue.KeyValueGraph")
	    		<br><span style="color:gray">$messagingtargetbypath.value.graph.keyValueStore.class.canonicalName</span>
		    	#if($messagingtargetbypath.value.graph.keyValueStore.class.canonicalName=="xdi2.core.impl.keyvalue.properties.PropertiesKeyValueStore")
		    		<br>[path] <span style="color:gray">$messagingtargetbypath.value.graph.keyValueStore.path</span>
		    	#end
		    	#if($messagingtargetbypath.value.graph.keyValueStore.class.canonicalName=="xdi2.core.impl.keyvalue.bdb.BDBKeyValueStore")
		    		<br>[databasePath] <span style="color:gray">$messagingtargetbypath.value.graph.keyValueStore.databasePath</span>
		    		<br>[databaseName] <span style="color:gray">$messagingtargetbypath.value.graph.keyValueStore.databaseName</span>
		    	#end
	    	#end
	    	#if($messagingtargetbypath.value.graph.class.canonicalName=="xdi2.core.impl.wrapped.WrappedGraph")
	    		<br><span style="color:gray">$messagingtargetbypath.value.graph.graphWrapper.class.canonicalName</span>
		    	#if($messagingtargetbypath.value.graph.graphWrapper.class.canonicalName=="xdi2.core.impl.wrapped.file.FileGraphWrapper")
		    		<br>[path] <span style="color:gray">$messagingtargetbypath.value.graph.graphWrapper.path</span>
		    	#end
		    	#if($messagingtargetbypath.value.graph.graphWrapper.class.canonicalName=="xdi2.core.impl.wrapped.url.URLGraphWrapper")
		    		<br>[url] <span style="color:gray">$messagingtargetbypath.value.graph.graphWrapper.url</span>
		    	#end
		    	#if($messagingtargetbypath.value.graph.graphWrapper.class.canonicalName=="xdi2.core.impl.wrapped.classpath.ClasspathGraphWrapper")
		    		<br>[classpath] <span style="color:gray">$messagingtargetbypath.value.graph.graphWrapper.classpath</span>
		    	#end
	    	#end
			<p><form action="" method="post">
				<input type="hidden" name="cmd" value="edit_messaging_target">
				<input type="hidden" name="messagingtargetpath" value="$messagingtargetbypath.key">
				<input type="submit" value="edit">
			</form></p>
    	#end
    	#if($messagingtargetbypath.value.class.canonicalName=="xdi2.messaging.target.impl.file.FileMessagingTarget")
    		<br>[path] <span style="color:gray">$messagingtargetbypath.value.path</span>
    		<br>[mimeType] <span style="color:gray">$messagingtargetbypath.value.mimeType</span>
    	#end
    </td>
    <td valign="top">
    	#if($messagingtargetbypath.value.ownerAuthority)
    		$messagingtargetbypath.value.ownerAuthority
    	#end
    </td>
	<td valign="top">
	#foreach($contributorlist in $messagingtargetbypath.value.contributors.entrySet())
		#foreach($contributor in $contributorlist.value)
			$contributorlist.key<br>$contributor.class.canonicalName<br><hr>
		#end
	#end
	</td>
	<td valign="top">
	#foreach($interceptor in $messagingtargetbypath.value.interceptors) 
		$interceptor.class.canonicalName<br>
	#end
	</td>
    </tr>
#end
</table>
<hr noshade>
<h1>Mounted MessagingTargetFactorys:</h1>
<table>
	<tr>
	<th>Path</th>
	<th>MessagingTargetFactory</th>
	</tr>
#foreach($messagingtargetfactorybypath in $messagingtargetfactorysbypath.entrySet())
	<tr>
    <td valign="top">
    	<a href="$requestinfo.contextPath$requestinfo.servletPath$messagingtargetfactorybypath.key">
    		$requestinfo.contextPath$requestinfo.servletPath$messagingtargetfactorybypath.key
    	</a>
		<p><form action="" method="post">
			<input type="hidden" name="cmd" value="unmount_messaging_target_factory">
			<input type="hidden" name="messagingtargetfactorypath" value="$messagingtargetfactorybypath.key">
			<input type="submit" value="unmount">
		</form></p>
    </td>
    <td valign="top">
    	$messagingtargetfactorybypath.value.class.canonicalName
    </td>
    </tr>
#end
</table>
<hr noshade>
See <a href="http://xdi2.projectdanube.org/">http://xdi2.projectdanube.org/</a> for more information.
</body>
</html>
